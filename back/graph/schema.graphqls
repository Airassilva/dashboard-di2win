# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Count {
  name: String!
  value: Int!
}

type User {
  id: ID!
  name: String!
  segment: String!
}

type Query {
  count(group_by: String!, user_id: Int, tipo_documento: String, data_comeco: String, data_final: String): [Count!]
  """
  extract(user_id: Int, tipo_documento: String, data_comeco: String, data_final: String): Contagem!
  """
  user(id: Int, segment: String): [User!]! 
}
